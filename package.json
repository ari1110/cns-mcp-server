{
  "name": "cns-mcp-server",
  "version": "1.0.0",
  "description": "Central Nervous System MCP Server for Autonomous Multi-Agent Orchestration with free local embeddings",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "cns-mcp-server": "./dist/index.js",
    "cns-server": "./dist/cli/server.js",
    "cns-client": "./dist/cli/client.js"
  },
  "files": [
    "dist",
    "README.md",
    "INSTALLATION.md",
    "USER-GUIDE.md",
    "TROUBLESHOOTING.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:production": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:integration": "npm run build && node scripts/test-embedding.js",
    "validate:production": "npm run build && node dist/cli/server.js validate",
    "lint": "eslint src",
    "prepublish": "node scripts/prepare-publish.js",
    "prepublishOnly": "npm run build:production && npm test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "orchestration",
    "multi-agent",
    "claude",
    "claude-code",
    "autonomous",
    "ai",
    "workflow",
    "embeddings",
    "transformers",
    "git-worktree",
    "semantic-search"
  ],
  "author": "CNS Team",
  "homepage": "https://github.com/ari1110/cns-mcp-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ari1110/cns-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/ari1110/cns-mcp-server/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@lancedb/lancedb": "latest",
    "@modelcontextprotocol/sdk": "latest",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "latest",
    "dotenv": "latest",
    "node-cron": "latest",
    "redis": "latest",
    "simple-git": "latest",
    "uuid": "latest",
    "winston": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/better-sqlite3": "latest",
    "@types/node": "latest",
    "@types/uuid": "latest",
    "audit-ci": "^6.6.1",
    "eslint": "latest",
    "globals": "^16.3.0",
    "semantic-release": "^21.0.0",
    "tsx": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest",
    "vitest": "latest"
  }
}
